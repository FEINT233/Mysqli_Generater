<?xml version="1.0" encoding="UTF-8"?>
<database name="sakila">
	<!-- 
	namerule 在生成po类时中表字段名的命名规则 可选none不使用,camelCase驼峰命名法
	默认为none不使用 
	不过更加推荐使用camelCase驼峰命名法 数据库字段起名也推荐使用student_name,student_sex 
	-->
	<table name="actor" namerule="camelCase"><!-- 表 name是名字 -->
		<field name="actor_id" isPK="TRUE"></field><!-- 字段 name是名字 -->
		<field name="first_name" ></field>
		<field name="last_update" ></field>
		<field name="last_name" ></field>
	</table>

	<table name="film_actor" namerule="camelCase"><!-- 表 name是名字 -->
		<field name="actor_id" isPK="TRUE"></field><!-- 字段 name是名字 -->
		<field name="film_id" isPK="TRUE"></field>
		<field name="last_update" ></field>
	</table>
</database>